<!--
This file contains the declaration of all the localizable strings.
-->
<WixLocalization xmlns="http://wixtoolset.org/schemas/v4/wxl" Culture="en-US">
	<?include Config.wxi?>
	
	<String Id="DowngradeError" Value="A newer version of $(var.ProductShortName) is already installed." />

	<String Id="WelcomeEulaDlg_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />	
	<String Id="WelcomeEulaDlgTitle" Value="{\Custom_Style}Please read the $(var.ProductShortName) License Agreement." />

	<String Id="ProgressDlg_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />
	<String Id="ProgressDlgTitleInstalling" Value="{\WixUI_Font_Title}Installing $(var.ProductShortName)." />
		
	<String Id="UserExit_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />	
