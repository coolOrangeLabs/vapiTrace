	<String Id="ExitDialog_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />
	<String Id="ExitDialogTitle" Value="{\WixUI_Font_Title}$(var.ProductShortName) Setup finished." />
	
	<String Id="MaintenanceWelcomeDlg_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />
	<String Id="MaintenanceWelcomeDlgTitle" Value="{\WixUI_Font_Title}Welcome to the setup for $(var.ProductShortName)." />

	<String Id="ResumeDlg_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />	
