	<String Id="VerifyReadyDlgRemove_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />
	<String Id="VerifyReadyDlgRemoveTitle" Value="{\WixUI_Font_Title}Ready to remove $(var.ProductShortName)" />

	<String Id="VerifyReadyDlgRepair_Title" Value="$(var.ProductShortName) [ProductVersion] Setup" />
